{
  "name": "Gerrit Commit Message Auto Correction",
  "version": "1.0.0.6",
  "manifest_version": 2,
  "description": "cherry-pick及代码合入仓库时自动修正commit message中的分支和版本信息",
  "browser_action": {
    "default_title": "gerrit-plugin"
  },
  "content_scripts":
  [
    {
      "matches": [
        "https://192.168.120.245/*",
        "https://192.168.120.246/*"
      ],
      "js": ["debug.js", "main.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "webRequest",
    "webRequestBlocking",
    "storage"
  ]
}
