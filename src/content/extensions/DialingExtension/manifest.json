{
  "name": "GRP Cross-platform Dialing",
  "version": "1.0.0.9",
  "description": "GRP cross-platform office account Dialing and DingTalk webpage auto full screen",
  "browser_action":
  {
    "default_title": "GRP Cross-platform Dialing",
    "default_icon": "popup/logo.png",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background/indexedDB.js",
      "background/xml2json.js",
      "background/background.js",
      "background/gs.sdk.min.js"
    ],
    "persistent": true
  },
  "content_scripts":
  [
    {
      "matches": [
        "https://im.dingtalk.com/*",
        "https://app.slack.com/*",
        "*://mail.qiye.163.com/*",
        "*://mail.grandstream.cn/*",
        "*://outlook.live.com/*"
      ],
      "js": [
        "contentScripts/injectScript.js",
        "contentScripts/contentScript.js"
      ],
      "css": [
        "contentScripts/contentScript.css"
      ],
      "all_frames": true,
      "match_about_blank": true,
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "cookies",
    "tabs",
    "<all_urls>"
  ],
  "manifest_version": 2
}
