{
  "name": "DingTalkExtension",
  "version": "1.0.0",
  "description": "钉钉布局扩展到浏览器窗口大小并设置话机一键呼叫",
  "browser_action":
  {
    "default_title": "DingTalk Extension",
    "default_icon": "popup/oldIcon.ico",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background/background.js",
      "background/gs.sdk.min.js"
    ],
    "persistent": true
  },
  "content_scripts":
  [
    {
      "matches": [
        "https://im.dingtalk.com/*"
      ],
      "js": [
        "contentScripts/extension.js",
        "contentScripts/socket.js"
      ],
      "css": [
        "contentScripts/extension.css"
      ],
      "all_frames": true,
      "match_about_blank": true,
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "cookies",
    "tabs",
    "<all_urls>",
    "activeTab",
    "*://*/*"
  ],
  "externally_connectable": {
    "matches": [
      "*://im.dingtalk.com/*"
    ]
  },
  "manifest_version": 2
}
