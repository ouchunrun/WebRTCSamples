{
  "name": "DingTalkExtension",
  "version": "2.0.1",
  "description": "办公号码一键呼叫和钉钉窗口自动全屏",
  "browser_action":
  {
    "default_title": "DingTalk Extension",
    "default_icon": "popup/logo.png",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background/background.js",
      "background/gs.sdk.min.js"
    ],
    "persistent": true
  },
  "content_scripts":
  [
    {
      "matches": [
        "https://im.dingtalk.com/*"
      ],
      "js": [
        "contentScripts/injectScript.js",
        "contentScripts/contentScript.js"
      ],
      "css": [
        "contentScripts/contentScript.css"
      ],
      "all_frames": true,
      "match_about_blank": true,
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "cookies",
    "tabs",
    "<all_urls>"
  ],
  "manifest_version": 2
}
