{
  "name": "Click2Dial",
  "version": "2.0.7",
  "description": "web版钉钉网页自动全屏和第三方即时通信系统的话机浏览器扩展支持(Chrome)",
  "browser_action":
  {
    "default_title": "Click2Dial",
    "default_icon": "popup/logo.png",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background/background.js",
      "background/gs.sdk.min.js"
    ],
    "persistent": true
  },
  "content_scripts":
  [
    {
      "matches": [
        "https://im.dingtalk.com/*",
        "https://app.slack.com/*"
      ],
      "js": [
        "contentScripts/injectScript.js",
        "contentScripts/contentScript.js"
      ],
      "css": [
        "contentScripts/contentScript.css"
      ],
      "all_frames": true,
      "match_about_blank": true,
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "cookies",
    "tabs",
    "<all_urls>"
  ],
  "manifest_version": 2
}
